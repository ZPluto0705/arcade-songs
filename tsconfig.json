{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@types/canvas-confetti",
      "@nuxt/types",
      "@nuxt/content",
      "@nuxtjs/i18n",
      "@nuxtjs/axios",
      "@nuxtjs/vuetify",
      "@nuxtjs/sentry",
      "@pinia/nuxt",
      "unplugin-vue2-script-setup/types"
    ]
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
